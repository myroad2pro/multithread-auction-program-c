Server-side:
	struct Product{
		String ProductName
		String ProductDescription
		User Seller
		User current_bidder
		int CurrentHighestPrice
	};
	
	struct ProductNode{
            struct Product product;
            struct ProductNode *next_Product;
	};

	struct Room{
		struct ProductNode *queue;		
		int room_number;
	};

